RewriteEngine On
RewriteRule ^([-a-zA-Z0-9]+).html$ index.php?page=$1&%{QUERY_STRING}
RewriteRule cart/checkout.html$  index.php?page=cart&checkout=true&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/88([0-9]+).html$ index.php?page=detail-news&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/2015([0-9]+).html$ index.php?page=detail-account&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/2016([0-9]+).html$ index.php?page=change-pass&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/2017([0-9]+).html$ index.php?page=change-avatar&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/66([0-9]+).html$ index.php?page=news&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/11([0-9]+).html$ index.php?page=detail-test&id=$2&%{QUERY_STRING}
RewriteRule ^([-a-zA-Z0-9]+)/22([0-9]+).html$ index.php?page=test&id=$2&%{QUERY_STRING}



